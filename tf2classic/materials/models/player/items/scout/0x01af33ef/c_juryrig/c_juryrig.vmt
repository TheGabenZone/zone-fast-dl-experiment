"VertexLitGeneric"
{
	"$baseTexture" "models\player\items\scout\0x01af33ef\c_juryrig\c_juryrig"
	"$bumpmap" "models\player\items\scout\0x01af33ef\c_juryrig\c_juryrig_normal"
	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "2"	





	"$envmap" env_cubemap
	"$envmaptint" "[0.2 0.2 0.2]"
	






//	"$phongexponent" "20"
//	"$phongboost" "3"	


//	"$lightwarptexture"		"models/player/pyro/pyro_lightwarp"
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"

	"$phongfresnelranges"	"[1 1 1]"
	"$halflambert" "1"



	// Rim lighting parameters
//	"$rimlight" "1"			
//	"$rimlightexponent" "4"	
//	"$rimlightboost" ".5"
//	"$rimmask" "1"

	"360?$color2" "[ 0.5 0.5 0.5 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
