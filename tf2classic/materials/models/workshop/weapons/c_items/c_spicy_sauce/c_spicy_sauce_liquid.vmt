"VertexlitGeneric"
{
	"$basetexture" "models/workshop/weapons/c_items/c_spicy_sauce/c_spicy_sauce"
	"$bumpmap" "models/workshop/weapons/c_items/c_spicy_sauce/c_spicy_sauce_phong"
	"$selfillummask" "models/workshop/weapons/c_items/c_spicy_sauce/c_spicy_sauce_illum"
	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "1.15"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.5 .5 3]"
	"$halflambert" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.06 .03 0]"

	"$selfillum" "1"
	"$selfillumtint" "[800 800 800]"

	// Rim lighting parameters
	"$rimlight" "1"			
	"$rimlightexponent" "6"	
	"$rimlightboost" ".5"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"Sine"
		{
			"sineperiod" "5"
			"sinemin" "1"
			"sinemax" "3"
			"resultVar" "$selfillumtint"
		}
	
	}
}















