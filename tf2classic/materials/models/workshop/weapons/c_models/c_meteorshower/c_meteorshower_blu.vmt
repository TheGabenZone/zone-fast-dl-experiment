"VertexLitGeneric"
{
	"$basetexture"	"models\workshop\weapons\c_models\c_meteorshower\c_meteorshower_blu"

	"$bumpmap" "models\workshop\weapons\c_models\c_meteorshower\c_meteorshower_normal"

	"$envmap" env_cubemap
	"$envmaptint" "[.05 .05 .05]"


	"$phong" "1"
	"$phongexponent" "12"
	"$phongboost" "3.5"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
//	"$lightwarptexture"		"models\workshop\weapons\c_models\c_meteorshower\foil_lightwarp"

	"$phongfresnelranges"	"[1 .5 .25]"
	"$halflambert" "1"

//	"$basemapalphaphongmask" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "1"



	"360?$color2" "[ 0.5 0.5 0.5 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
