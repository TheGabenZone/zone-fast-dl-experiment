"VertexlitGeneric"
{
	"$basetexture" "models/workshop/weapons/c_models/c_barrel_minigun/c_barrel_minigun"

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "0.2"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$selfillum" "1"

	"$selfillummaskscale" "1000"


	"$glowcolor" "1"


	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		"Sine"
		{
			"sineperiod" ".5"
			"sinemin" 1
			"sinemax" 1
			"resultVar" "$selfillumtint"
		}
		"Sine"
		{
			"sineperiod" "1"
			"sinemin" .8
			"sinemax" 1
			"resultVar" "$selfillumtint"
		}
		"Sine"
		{
			"sineperiod" "1"
			"sinemin" .8
			"sinemax" 1
			"resultVar" "$selfillumtint"
		}
		
	}
}
