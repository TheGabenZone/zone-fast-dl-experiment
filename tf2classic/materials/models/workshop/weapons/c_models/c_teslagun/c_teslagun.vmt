"VertexlitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_teslagun/c_teslagun.vtf"
	"$envmap" "env_cubemap"
	"$envmapmask" "models/workshop/weapons/c_models/c_teslagun/c_teslagun_envmap.vtf"
	"$envmaptint" "[.1 .1 .1]"

	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "3"	
	"$lightwarptexture" "models\lightwarps\softened_weapon_lightwarp"
	"$phongfresnelranges"	"[1.0 .5 3]"
  //"$phongfresnelranges"	"[.5 .5 1]"
	"$halflambert" "1"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".25"
  
  "$selfillum" "1"
  "$selfillumtint" "[3 3 3]"
  "$selfillummask" "models/workshop/weapons/c_models/c_teslagun/c_teslagun_illum.vtf"

	"360?$color2" "[ 0.5 0.5 0.5 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
