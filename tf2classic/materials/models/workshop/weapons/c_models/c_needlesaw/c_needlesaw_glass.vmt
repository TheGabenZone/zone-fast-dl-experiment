"VertexlitGeneric"
{
	"$basetexture" "models/workshop/weapons/c_models/c_needlesaw/c_needlesaw"
	"$additive" "1"
	"$basemapalphaphongmask" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.3 .3 .7]"

	"$phong" "1"
	"$phongboost" "2.5"
	"$phongexponent" "65"
	"$phongfresnelranges"	"[.5 .5 3]"
	"$lightwarptexture" "models/lightwarps/jarate_lightwarp"
	"$halflambert" "1"

	"$rimlight" "1"			
	"$rimlightexponent" "6"	
	"$rimlightboost" ".5"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	}
}




























