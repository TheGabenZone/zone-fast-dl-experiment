"VertexLitGeneric"
{
 	"$basetexture" "models/workshop/weapons/c_models/c_cricket_bat/cricket_ball"
	"$bumpmap" "models/workshop/weapons/c_models/c_cricket_bat/cricket_ball_normal"

	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "10"	
	"$lightwarptexture" "models\player\pyro\pyro_lightwarp"
	"$phongfresnelranges"	"[.25 .15 .05]"

	"$rimlight" "1"
	"$rimlightexponent" "5"	
	"$rimlightboost" "1"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}

}
