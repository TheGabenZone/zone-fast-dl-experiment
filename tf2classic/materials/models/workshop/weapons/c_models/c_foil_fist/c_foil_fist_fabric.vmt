"VertexLitGeneric"
{
	"$basetexture"	"models\workshop\weapons\c_models\c_foil_fist\c_foil_fist"

	"$bumpmap" "models\workshop\weapons\c_models\c_foil_fist\c_foil_fist_normal"

//	"$PhongExponentTexture" "models\workshop\weapons\c_models\c_meteorshower\c_meteorshower_exponent"



//	"$phongalbedotint" 1




	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" ".2"	




	"$lightwarptexture"		"models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[1 0 15]"

	"$rimlight" "1"
	"$rimlightexponent" "8"	
	"$rimlightboost" ".01"



//	"$envmap" "models\workshop\weapons\c_models\c_foil_fist\foil_cubemap"

//	"$envmaptint" "[0.4 .38 .35]"


//	"$envmap"	env_cubemap
//	"$basealphaenvmapmask"		"0"
//	"$envmaptint"		"[0.01 0.01 0.01]"










//	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"


	"$halflambert" "1"

//	"$basemapalphaphongmask" "1"



	"360?$color2" "[ 0.5 0.5 0.5 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
