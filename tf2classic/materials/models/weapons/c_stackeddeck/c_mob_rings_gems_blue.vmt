"VertexLitGeneric"
{
	"$basetexture"	"models\weapons\c_stackeddeck\c_mob_rings_blue"

	"$basemapalphaphongmask" "1"

	"$phong" "1"
	"$phongexponent" "30"
	"$phongboost" "6"
	"$phongalbedotint" "1"

	"$phongtint" "[0.1 0.8 0.4]"
	

	
	"$lightwarptexture" "models/lightwarps/softened_weapon_lightwarp"
	
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"
	"$glowcolor" "1"
	

	"$envmap" "cubemaps\cubemap_specular001"
	"$envmaptint" "[.05 .1 .1]"

	
	"$selfillum" 1
	"$selfillumtint" "[.4 .2 .2]"
	"$selfIllumFresnel" "1"
	"$selfIllumFresnelMinMaxExp" "[0 10 20]"



	"$tempvar" "0"

	"$rimlight" "1"
	"$rimlightexponent" "10"
	"$rimlightboost" "2"


	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ .2 .2 .5 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"WeaponSkin"
		{
		}
	}
}
