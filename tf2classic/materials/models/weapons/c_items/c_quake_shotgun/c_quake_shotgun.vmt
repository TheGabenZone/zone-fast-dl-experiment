"VertexLitGeneric"
{
	"$baseTexture" "models\weapons\c_items\c_quake_shotgun\c_quake_shotgun.vtf"
	"$basemapalphaenvmapmask" "1"
	"$basemapalphaphongmask" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.3 .3 .3]"

	"$phong" "1"
	"$phongexponent" "24"
	"$phongboost" "4.8"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$rimlight" "1"
	"$rimlightexponent" "12"	
	"$rimlightboost" "0.6"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}