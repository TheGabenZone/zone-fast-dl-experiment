"VertexLitGeneric"
{
   	 "$basetexture"          "models\weapons\angelBlade\angelBlade_blue_D"

    	"$bumpmap"          "models\weapons\angelBlade\angelBlade_N"

	"$phong"		"1"
	"$phongexponent"		"1.000000"
	"$phongboost"		"1.000000"
	"$phongfresnelranges"		"[.25 .5 1]"
	"$rimlight"		"1"
	"$rimlightexponent"		"4"
	"$rimlightboost"		"2"
	"$BumpmapAlphaPhongMask"	"1"

	"$lightwarptexture" "models\weapons\c_items\c_ambassador_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$phongwarptexture"  "models\weapons\c_items\c_ambassador_phongwarp"
	
    "$rimlight"             "1"     
    "$rimlightexponent"     "4"      
    "$rimlightboost"        "2"    
 
	

	// Cloaking

	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}

}

