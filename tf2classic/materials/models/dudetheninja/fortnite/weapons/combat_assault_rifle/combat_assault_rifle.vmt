"VertexLitGeneric"
{
	"$basetexture"	"\models\dudetheninja\fortnite\weapons\combat_assault_rifle\T_Weapon_Branch_AR_D"
	"$bumpmap"	"\models\dudetheninja\fortnite\weapons\combat_assault_rifle\T_Weapon_Branch_AR_N"
	"$Phong" "1"
	"$PhongBoost" "2" 
	"$phongexponent" 15
	//"$PhongExponentTexture" "\models\dudetheninja\fortnite\weapons\combat_assault_rifle\T_SMG_Retro_Sci_Fi_MC" 
	//"$phongwarptexture"  "models/weapons/c_models/c_ambassador/c_ambassador_lightwarp"

	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.3 .5 3]"
	"$halflambert" "1"
	"$glowcolor" "1"
	
	"$envmap" "env_cubemap"
	"$envmaptint" "[.05 .05 .05]"
	
	// Rim lighting parameters
	"$rimlight" "1"			
	"$rimlightexponent" "6"	
	"$rimlightboost" "1"

	"360?$color2" "[ 0.5 0.5 0.5 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
