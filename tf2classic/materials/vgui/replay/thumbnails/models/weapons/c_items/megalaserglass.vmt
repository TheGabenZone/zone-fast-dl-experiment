"VertexlitGeneric"
{
	"$baseTexture" "vgui\replay\thumbnails\models\weapons\c_items\megalaser"
	"$bumpmap" "models/weapons/c_items/c_urinejar_glass_phongmask"
	"$additive"	1

	"$envmap" "env_cubemap"
	"$envmaptint" "[.6 .6 .4]"

	"$phong" "1"
	"$phongexponent" "65"
	"$phongfresnelranges" "[1 2 4]"
	"$lightwarptexture" "models/lightwarps/jarate_lightwarp"
	"$phongboost" "6"

	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" "2"
	"$glowcolor" "1"
	
    "Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}
