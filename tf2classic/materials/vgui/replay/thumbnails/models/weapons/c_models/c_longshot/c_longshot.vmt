"VertexLitGeneric"
{
	"$basetexture"	"vgui\replay\thumbnails\models/weapons/c_models/c_longshot/c_longshot"
//	"$bumpmap" "vgui\replay\thumbnails\models/weapons/c_models/c_longshot/c_longshot_normal"
	"$phongexponenttexture" "vgui\replay\thumbnails\models/weapons/c_models/c_longshot/c_longshot_exp"
	
	"$phong" "1"
	//comment out to use the exponent mask
//	"$phongexponent" "4"
	"$phongboost" "20"	
	
	//scales up the exponent texture which usually only has about 120 levels of exponent
	"$phongexponentfactor"  "255"
	"$phongalbedotint" "1"
	
	"$lightwarptexture" "models/lightwarps/softened_weapon_lightwarp"
			
	"$phongfresnelranges"	"[.25 .5 1]"
	"$halflambert" "1"
	"$glowcolor" "1"
	
	"$envmap" "env_cubemap"
	"$envmaptint" "[ 0.015686 0.035294 0.035294 ]"
	"$envmapcontrast" "0.5"
	
	"$tempvar" "0"

	//comment out to use the exponent mask
	"$basephongexponent" "4"
	"$basephongboost" "1"

	"$basealphaenvmapmask" "1"
	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"						
	"$rimlightexponent" "1"				
	"$rimlightboost" "1.5"
//	"$rimmask" "1"
	

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{

		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
